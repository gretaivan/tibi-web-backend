{
  "name": "tibi-web-backend",
  "version": "1.0.0",
  "description": "Launch teaser website to collect people interest on the product",
  "main": "app.js",
  "scripts": {
    "test": "jest --setupFiles ./__tests__/integration/config.js --watchAll --detectOpenHandles --runInBand --verbose",
    "dev": "nodemon -r dotenv/config app.js --legacy-watch",
    "start": "node -r dotenv/config app.js",
    "seedDev": "node dbConfig/seedDev.js",
    "unitTests": "jest --testPathPattern=/unit/ --watchAll --verbose",
    "coverage": "jest --setupFiles ./__tests__/integration/config.js --testPathPattern=/unit/ --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/integration/config.js",
      "/dbConfig/init.js"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/integration/config.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gretaivan/tibi-web.git"
  },
  "keywords": [
    "website",
    "javascript",
    "node.js",
    "jest",
    "nodemon",
    "heroku"
  ],
  "author": "Greta Ivan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gretaivan/tibi-web/issues"
  },
  "homepage": "https://github.com/gretaivan/tibi-web#readme",
  "devDependencies": {
    "fs": "0.0.1-security",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.4.1",
    "pg": "^8.7.1"
  }
}
